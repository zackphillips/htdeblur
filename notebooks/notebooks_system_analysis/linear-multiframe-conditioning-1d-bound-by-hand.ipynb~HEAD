   "source": [
    "illumination = gen_illum(blur_length, 0.5, illum_type = 'pure_rand')\n",
    "blur_matrix = blur_matrix_1d(illumination, object_height)\n",
    "\n",
    "pipo_offset = int(blur_length/2)\n",
    "crop_pipo = crop_operator(object_height+blur_length, object_height+2, offset=pipo_offset)\n",
    "\n",
    "cropped_blur_matrix = crop_pipo.dot(blur_matrix)\n",
    "n,m = cropped_blur_matrix.shape\n",
    "\n",
    "# all unique illumination patterns to consider (TODO check that this is correct)\n",
    "psfs = []\n",
    "for i in range(pipo_offset,0,-1):\n",
    "    psfs.append(np.hstack([illumination[i:], np.zeros(i)]))\n",
    "psfs.append(illumination)\n",
    "tail_offset = object_height+blur_length - object_height+1 - pipo_offset\n",
    "for i in range(1,tail_offset):\n",

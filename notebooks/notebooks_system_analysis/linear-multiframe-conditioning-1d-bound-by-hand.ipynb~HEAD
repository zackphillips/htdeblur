    "plt.subplot(1,3,1); plt.imshow(crop); plt.title('crop matrix')\n",
    "plt.subplot(1,3,2); plt.imshow(crop.dot(blur_matrix)); plt.title('forward operator')\n",
    "plt.subplot(1,3,3); plt.imshow(crop.dot(blur_matrix.dot(object_true)), cmap='gray_r'); plt.title('image')\n",
    "\n",
    "# without adding one, we get a non-invertible operator, possibly to do with rounding\n",
    "crop_pipo = crop_operator(object_height+blur_length, object_height+2, offset=int(blur_length/2))\n",
    "\n",
    "plt.figure(figsize=(9,3))\n",
    "plt.subplot(1,3,1); plt.imshow(crop_pipo); plt.title('crop matrix')\n",
    "plt.subplot(1,3,2); plt.imshow(crop_pipo.dot(blur_matrix)); plt.title('pixel-in-pixel-out forward operator')\n",
    "plt.subplot(1,3,3); plt.imshow(crop_pipo.dot(blur_matrix.dot(object_true)), cmap='gray_r'); plt.title('image')\n",
    "\n",
    "\n",
    "u, s, v = np.linalg.svd(blur_matrix)\n",
    "plt.figure(figsize=(4,2)); plt.semilogy(np.sort(s**2), label='full blur'); \n",
    "\n",
    "Fb = np.fft.fft(np.hstack([illumination, np.zeros(object_height)]))\n",
    "bHb = np.conj(Fb) * Fb\n",
    "plt.semilogy(np.sort(bHb), label='Ft illum');\n",
    "\n",
    "u, s, v = np.linalg.svd(crop_pipo.dot(blur_matrix))\n",
    "plt.semilogy(np.sort(s**2), label='cropped blur');  plt.legend()\n",
    "plt.title(\"(sorted) squared singular values\")\n",
    "\n"
   ]

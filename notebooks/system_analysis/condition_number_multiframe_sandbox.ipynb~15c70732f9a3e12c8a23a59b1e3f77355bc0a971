    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Condition number of A: 1.0000 \n",
      "Condition number of D: 36.1457 \n",
      "Left multiply condition number: 36.1457 \n",
      "Right multiply condition number: 36.1457 \n",
      "Left multiply condition number: 36.1457 \n",
      "Left and Right same multiply condition number: 1306.5087  (square root: 36.1457)\n",
      "-----\n",
      "Condition number of D_2: 59.4942 \n",
      "Condition number of D_2 * D: 218.6274 \n",
      "Left and Right different multiply condition number: 218.6273\n"
     ]
    }
   ],
   "source": [
    "object_size = (25,25)\n",
    "image_size = (10,10)\n",
    "\n",
    "# Explination for left multiply\n",
    "FF = ops.FourierTransform(object_size).asFullMatrix()\n",
    "b = np.random.rand(object_size[0] * object_size[1]) +  np.random.rand(object_size[0] * object_size[1]) * 1j\n",
    "DD = ops.Diagonalize(b).asFullMatrix()\n",
    "\n",
    "b_2 = np.random.rand(object_size[0] * object_size[1]) +  np.random.rand(object_size[0] * object_size[1]) * 1j\n",
    "DD_2 = ops.Diagonalize(b_2).asFullMatrix()\n",
    "\n",
    "FF = ops.FourierTransform(object_size).asFullMatrix()\n",
    "print('Condition number of A: %.4f ' % np.linalg.cond(FF))\n",
    "print('Condition number of D: %.4f ' % np.linalg.cond(DD))\n",
    "print('Left multiply condition number: %.4f ' % np.linalg.cond(DD * FF))\n",
    "print('Right multiply condition number: %.4f ' % np.linalg.cond(FF * DD))\n",
    "print('Left multiply condition number: %.4f ' % np.linalg.cond(DD * FF))\n",
    "print('Left and Right same multiply condition number: %.4f  (square root: %.4f)' % (np.linalg.cond(DD * FF * DD), math.sqrt(np.linalg.cond(DD * FF * DD))))\n",
    "\n",
    "print('-----')\n",
    "print('Condition number of D_2: %.4f ' % np.linalg.cond(DD_2))\n",
    "print('Condition number of D_2 * D: %.4f ' % np.linalg.cond(DD_2 * DD))\n",
    "print('Left and Right different multiply condition number: %.4f' % np.linalg.cond(DD_2 * FF * DD))"
   ]

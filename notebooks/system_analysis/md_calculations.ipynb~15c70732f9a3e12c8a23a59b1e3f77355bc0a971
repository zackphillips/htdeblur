   ],
   "source": [
    "update_speed_range = np.flip(np.logspace(-3, 2), axis=0) / 1000# ms\n",
    "\n",
    "# Define parameters\n",
    "sample_size = [25., 75] # mm\n",
    "\n",
    "# Velocity of stage\n",
    "v_max = 20 # mm/s\n",
    "\n",
    "ps = 6.5 #um\n",
    "mag = 2 * 20\n",
    "ps_eff = ps / mag #um\n",
    "n_px = np.asarray([2100, 2500])\n",
    "fov = n_px * ps_eff\n",
    "\n",
    "t_settle = 0.1   #s\n",
    "t_ro     = 0.005 #s\n",
    "t_exp    = 0.01  #s\n",
    "n_frames = math.ceil((sample_size[0] * sample_size[1]) / (fov[0] / 1000. * fov[1] / 1000.))\n",
    "print('Acquisition would require %d frames' % n_frames)\n",
    "\n",
    "# Acceleration\n",
    "a = 10000 # m/s/s\n",
    "\n",
    "ratio_list = []\n",
    "for update_speed in update_speed_range:\n",
    "\n",
    "    # Max velocity of stage\n",
    "    v_max_ours = ps_eff / 1000. / update_speed # mm/s\n",
    "    \n",
    "    # Distance between each frame\n",
    "    x_mm = fov[0] / 1000 # mm\n",
    "\n",
    "    # Previous imaging time\n",
    "    t_prev = x_mm / v_max + v_max / a + t_settle + t_exp # s\n",
    "\n",
    "    # Motion deblur exposure time necessary to move one FOV\n",
    "    t_exp_md = x_mm / v_max_ours # s\n",
    "\n",
    "    # New imaging time\n",
    "    t_new = t_ro + t_exp_md # s\n",
    "\n",
    "    # Difference\n",
    "    difference = n_frames * (t_prev - t_new)\n",
    "\n",
    "    # Ratio\n",
    "    ratio = t_prev / t_new\n",
    "    \n",
    "    # List of ratios\n",
    "    ratio_list.append(ratio)\n",
    "    \n",
    "plt.style.use('seaborn-poster')\n",
    "plt.figure(figsize=(7,5))\n",
    "plt.semilogx(update_speed_range, ratio_list, c='b', label='Speed Ratio of our Method to Stop-and-Stare')\n",
    "ax = plt.gca()\n",
    "plt.plot(update_speed_range, np.ones(update_speed_range.shape), c='r', label='Unit Ratio')\n",
    "plt.xlabel('Illumination update speed (s)')\n",
    "plt.ylabel('Speed Ratio (Ours / Previous)')\n",
    "plt.title('Speed ratio - ' + str(int(mag/2)) + 'x, 20mm/s stage speed')\n",
    "plt.ylim((0,30))\n",
    "# plt.legend()\n",
    "plt.grid(True,which=\"both\",ls=\"-\")\n",
    "plt.tight_layout()\n",
    "plt.savefig('Speed ratio ' + str(int(mag/2)) + 'x, 20mm stage speed.png', transparent=True)"
   ]

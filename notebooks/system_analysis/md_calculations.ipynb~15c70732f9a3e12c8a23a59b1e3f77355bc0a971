   "source": [
    "scan_velocity_range = np.logspace(1e-3, 10)\n",
    "update_sapeed_range = np.flip(np.logspace(1e-3, 10), axis=0) # ms\n",
    "v, us = np.meshgrid(scan_velocity_range, update_speed_range)\n",
    "\n",
    "# First param is velocity, second is update speed\n",
    "params_to_scan = np.vstack((v.ravel(),us.ravel())).T\n",
    "\n",
    "# Define parameters\n",
    "sample_size = [25., 75] # mm\n",
    "\n",
    "ps = 6.5 #um\n",
    "mag = 2 * 20\n",
    "ps_eff = ps / mag #um\n",
    "n_px = np.asarray([2100, 2500])\n",
    "fov = n_px * ps_eff\n",
    "\n",
    "t_settle = 0.1   #s\n",
    "t_ro     = 0.005 #s\n",
    "t_exp    = 0.01  #s\n",
    "n_frames = math.ceil((sample_size[0] * sample_size[1]) / (fov[0] / 1000. * fov[1] / 1000.))\n",
    "print('Acquisition would require %d frames' % n_frames)\n",
    "\n",
    "# Acceleration\n",
    "a = 10000 # m/s/s\n",
    "\n",
    "ratio_list = []\n",
    "for param in params_to_scan:\n",
    "    \n",
    "    scan_velocity = param[0]\n",
    "    update_speed = param[1]\n",
    "\n",
    "    # Max velocity of stage\n",
    "    v_max_ours = ps_eff / 1000. / update_speed # mm/s\n",
    "    \n",
    "    # Velocity of stage\n",
    "    v_max = scan_velocity # mm/s\n",
    "\n",
    "    # Distance between each frame\n",
    "    x_mm = fov[0] / 1000 # mm\n",
    "\n",
    "    # Previous imaging time\n",
    "    t_prev = x_mm / v_max + v_max / a + t_settle + t_exp # s\n",
    "\n",
    "    # Motion deblur exposure time necessary to move one FOV\n",
    "    t_exp_md = x_mm / v_max_ours # s\n",
    "\n",
    "    # New imaging time\n",
    "    t_new = t_ro + t_exp_md # s\n",
    "\n",
    "    # Difference\n",
    "    difference = n_frames * (t_prev - t_new)\n",
    "\n",
    "    # Ratio\n",
    "    ratio = t_prev / t_new\n",
    "    \n",
    "    ratio_list.append(ratio)\n",
    "\n",
    "\n",
    "M = np.asarray(ratio_list).reshape(v.shape)\n",
    "ratio_list / max(ratio_list)\n",
    "plt.figure()\n",
    "plt.subplot(121)\n",
    "plt.imshow(M)\n",
    "\n",
    "plt.subplot(122)\n",
    "plt.scatter(params_to_scan[:,0], params_to_scan[:,1], c = (ratio_list))\n",
    "ax = plt.gca()\n",
    "ax.set_yscale('log')\n",
    "ax.set_xscale('log')\n"
   ]
  },
